<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobile_dev_project">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mobile_Dev_Project">
        <activity android:name=".Website"></activity>
        <activity android:name=".InfoPage" />
        <activity
            android:name=".Party"
            android:label="@string/Feesten_label" />
        <activity
            android:name=".Eten_Drinken"
            android:label="@string/label_horeca" />
        <activity
            android:name=".Bekende_Plaatsen"
            android:label="@string/label_toerisme" />
        <activity
            android:name=".NieuwsTablad"
            android:label="@string/label_nieuws" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>